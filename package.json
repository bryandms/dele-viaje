  "dependencies": {
    "apollo-server-express": "^2.4.0",
    "bcrypt": "^3.0.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "graphql": "^14.1.1",
    "jsonwebtoken": "^8.4.0",
